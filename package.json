{
  "name": "@ergou_rrrr/eslint-config",
  "type": "module",
  "version": "1.0.16",
  "packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a",
  "description": "Ray's ESLint config",
  "author": {
    "name": "Ray",
    "email": "rayguo.rf@gmail.com",
    "url": "https://github.com/RayGuo-ergou"
  },
  "license": "MIT",
  "homepage": "https://github.com/RayGuo-ergou/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean --dts",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build",
    "dev": "tsup src/index.ts --format esm,cjs --watch & npx @eslint/config-inspector",
    "stub": "tsup src/index.ts --format esm",
    "lint": "pnpm run stub && eslint .",
    "prepack": "nr build",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "2.19.1"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@ergou_rrrr/eslint-config": "workspace:*",
    "@eslint/config-inspector": "^0.5.1",
    "@types/node": "^20.14.9",
    "bumpp": "^9.4.1",
    "eslint": "^9.6.0",
    "jiti": "^1.21.6",
    "lint-staged": "^15.2.7",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
